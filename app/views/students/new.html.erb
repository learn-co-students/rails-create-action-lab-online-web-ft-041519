<!-- Create your form here -->
<h1> Student Form </h1>

<form action="<%= students_path %>" method="POST">
  
  <label>Student Names:</label><br>
  
  First name: <input type="text" id="first_name" name="first_name"><br>
  Last name: <input type="text" id="last_name" name="last_name"><br>
  
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  
  <input type="submit" id ="Submit Student" value="Submit Student">

</form>
